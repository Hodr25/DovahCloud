{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flask (debug)",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "app.py",
        "FLASK_DEBUG": "1",
        "FLASK_RUN_HOST": "0.0.0.0",
        "FLASK_RUN_PORT": "5000"
      },
      "args": ["run"]
    }
  ]
}
